	Issn = {87566222},
	Journal = {Journal of Law, Economics, and Organization},
	Mendeley-Groups = {American Bureaucracy/06. Control Problems},
	Number = {1},
	Pages = {1--29},
	Title = {{Political control and the power of the agent}},
	Volume = {22},
