@article{Baldwin2012,
	Abstract = {Local elites are thought to influence how other individuals vote in many new democracies. The existing literature suggests the influence of these leaders undermines the voting process by causing voters to support candidates for reasons tangential to political performance. However, voters who support candidates preferred by local patrons may be making sophisticated political inferences. In many developing countries, access to government-provided local public goods is mediated by local patrons, who are better able to deliver resources if they have strong relationships with elected politicians. This argument is tested using data from an original survey of traditional chiefs and an experiment involving voters in Zambia. Chiefs and politicians with stronger relationships collaborate more effectively to provide local public goods. Furthermore, voters are particularly likely to vote with their chief if they perceive the importance of chiefs and politicians working jointly for local development.},
	Author = {Healy, Andrew and Lenz, Gabriel S.},
	Date-Added = {2018-07-28 20:02:16 +0000},
